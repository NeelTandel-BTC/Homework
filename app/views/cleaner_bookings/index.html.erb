<table>
  <tr>
    <th>Customer name</th>
    <th>Booking Date</th>
    <th>Booking Time</th>
  </tr>
  <% if @bookings.present? %>
    <% @bookings.each do |booking| %>
      <tr>
        <td>
          <%= booking.customer.first_name %>
        </td>
        <td>
          <%= booking.start_time.strftime("%d/%m/%Y") %>
        </td>
        <td>
          <%= booking.end_time.strftime("%I:%M %p") %>
        </td>
      </tr>
    <% end %>
    <% end %>
</table>
<%= link_to 'Sign out', destroy_cleaner_session_path, method: 'delete' %>
